
// Example node to add and
template <typename T> node Math
{
  T a;
  T b;

  T add
  {
    return a + b;
  }

  T sub
  {
    return a - b;
  }

  T mul
  {
    return a * b;
  }
};

node Compound
{
  float source_flt;
  Math<float> source;

  Math<float> computed_thing
  {
    Math<float> out;
    out.a = source_flt;
    out.b = source.add;
    return out;
  }
};
